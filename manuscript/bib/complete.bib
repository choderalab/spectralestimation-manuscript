@article{Yi:2013vj,
author = {Yi, Zheng and Lindner, Benjamin and Prinz, Jan-Hendrik and No{\'e}, Frank and Smith, Jeremy C},
title = {{Dynamic Neutron Scattering from Conformational Dynamics II: Application using Molecular Dynamics Simulation and Markov modeling}},
journal = {J. Chem. Phys.},
year = {2013},
number = {submitted},
pages = {1--18},
month = jun,
rating = {0},
date-added = {2013-06-18T07:05:11GMT},
date-modified = {2013-10-26T16:35:33GMT},
abstract = {Neutron scattering experiments directly probe the dynamics of complex molecules on the sub pico- to microsecond time scales. However, the assignment of the relaxations seen experimentally to specific structural rearrangements is difficult, since many of the underlying dynamical processes may exist on similar timescales. In an accompanying article we present a theoretical approach to the analysis of molecular dynamics simulations with a Markov State Model (MSM) that permits the direct identification of structural transitions leading to each contributing relaxation process. Here, we demonstrate the use of the method by applying it to the configurational dynamics of the well-characterized alanine dipeptide. A practical procedure for deriving the MSM from an MD is introduced. The result is a 9-state MSM in the space of the backbone dihedral angles and the side- chain methyl group. The agreement between the quasielastic spectrum calculated directly from the atomic trajectories and that derived from the Markov state model is excellent. The dependence on the wavevector of the individual Markov processes is described. The procedure means that it is now practicable to interpret quasielastic scattering spectra in terms of well-defined intramolecular transitions with minimal a priori assumptions as to the nature of the dynamics taking place.},
file = {{Yi_2013_J. Chem. Phys._Dynamic Neutron Scattering from Conformational Dynamics II Application using Molecular Dynamics Simulation and Markov modeling.pdf:/Users/jan-hendrikprinz/Studium/Literature/Papers2/Files/Yi_2013_J. Chem. Phys._Dynamic Neutron Scattering from Conformational Dynamics II Application using Molecular Dynamics Simulation and Markov modeling.pdf:application/pdf}},
uri = {\url{papers2://publication/uuid/D7FE760F-AF92-4AE2-AD90-821F17348091}}
}

@article{Lindner:vm,
author = {Lindner, Benjamin and Yi, Zheng and Prinz, Jan-Hendrik and Smith, Jeremy C and No{\'e}, Frank},
title = {{Dynamic Neutron Scattering from Conformational Dynamics I: Theory and Markov models.}},
journal = {J. Chem. Phys.},
year = {2013},
pages = {1--17},
month = jun,
rating = {0},
date-added = {2013-06-18T07:08:44GMT},
date-modified = {2013-10-26T16:35:33GMT},
abstract = {The dynamics of complex molecules can be directly probed by inelastic neutron scattering experiments. However, many of the underlying dynamical processes may exist on similar timescales, which makes it difficult to assign processes seen experimentally to specific structural rearrangements. Here, we show how Markov models and can be used to connect structural changes observed in molecular dynamics simulation directly to the relaxation processes probed by scattering experiments. For this, a conformational dynamics theory of dynamical neutron and X-ray scattering is de- veloped, following our previous approach for computing dynamical fingerprints of time-correlation functions [1]. Markov modeling is used to approximate the relaxation processes and timescales of the molecule via the eigenvectors and eigenvalues of a transition matrix between conformational substates. This procedure allows the establishment of a complete set of exponential decay functions and a full decomposition into the individual contributions, i.e., the contribution of every atom and dynamical process to each experimental relaxation process.},
file = {{Lindner_2013_J. Chem. Phys._Dynamic Neutron Scattering from Conformational Dynamics I Theory and Markov models.pdf:/Users/jan-hendrikprinz/Studium/Literature/Papers2/Files/Lindner_2013_J. Chem. Phys._Dynamic Neutron Scattering from Conformational Dynamics I Theory and Markov models.pdf:application/pdf}},
