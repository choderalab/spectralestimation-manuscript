year = {2001},
volume = {311},
number = {2},
pages = {373--393},
month = aug,
affiliation = {Department of Chemistry and the Skaggs Institute for Chemical Biology, The Scripps Research Institute, 10550 North Torrey Pines Road, La Jolla, CA 92037, USA.},
