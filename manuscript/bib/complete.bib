@article{Cooke:2008jj,
author = {Cooke, Ben and Schmidler, Scott C},
title = {{Statistical prediction and molecular dynamics simulation.}},
journal = {Biophys. J.},
year = {2008},
volume = {95},
number = {10},
pages = {4497--4511},
