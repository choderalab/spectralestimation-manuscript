@article{Noe:2008tl,
author = {No{\'e}, Frank and Daidone, Isabella and Smith, Jeremy C and Di Nola, Alfredo and Amadei, Andrea},
title = {{Solvent Electrostriction-Driven Peptide Folding Revealed by Quasi-Gaussian Entropy Theory and Molecular Dynamics Simulation}},
