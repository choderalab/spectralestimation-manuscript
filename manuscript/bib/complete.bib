@article{Levitin:2012eo,
author = {Levitin, Daniel J. and Chordia, Parag and Menon, Vinod},
title = {{Musical rhythm spectra from Bach to Joplin obey a 1/f power law}},
journal = {PNAS Early Edition},
year = {2012},
month = feb,
doi = {10.1073/pnas.1113828109},
read = {Yes},
rating = {0},
date-added = {2012-02-22T14:10:44GMT},
date-modified = {2013-08-29T18:29:35GMT},
abstract = {Much of our enjoyment of music comes from its balance of predictability and surprise. Musical pitch fluctuations follow a 1/f power law that precisely achieves this balance. Musical rhythms, especially those of Western classical music, are considered highly regular and predictable, and this predictability has been hypothe- sized to underlie rhythm's contribution to our enjoyment of music. Are musical rhythms indeed entirely predictable and how do they vary with genre and composer? To answer this question, we ana- lyzed the rhythm spectra of 1,788 movements from 558 composi- tions of Western classical music. We found that an overwhelming majority of rhythms obeyed a 1/f $\beta$ power law across 16 subgenres and 40 composers, with $\beta$ ranging from ∼0.5--1. Notably, classical composers, whose compositions are known to exhibit nearly iden- tical 1/f pitch spectra, demonstrated distinctive 1/f rhythm spectra: Beethoven's rhythms were among the most predictable, and Mozart's among the least. Our finding of the ubiquity of 1/f rhythm spectra in compositions spanning nearly four centuries demonstrates that, as with musical pitch, musical rhythms also exhibit a balance of predictability and surprise that could contrib- ute in a fundamental way to our aesthetic experience of music. Although music compositions are intended to be performed, the fact that the notated rhythms follow a 1/f spectrum indicates that such structure is no mere artifact of performance or perception, but rather, exists within the written composition before the music is performed. Furthermore, composers systematically manipulate (consciously or otherwise) the predictability in 1/f rhythms to give their compositions unique identities.},
file = {{Levitin_2012_PNAS Early Edition_Musical rhythm spectra from Bach to Joplin obey a 1f power law.pdf:/Users/jan-hendrikprinz/Studium/Literature/Papers2/Files/Levitin_2012_PNAS Early Edition_Musical rhythm spectra from Bach to Joplin obey a 1f power law.pdf:application/pdf}},
uri = {\url{papers2://publication/doi/10.1073/pnas.1113828109}}
}

